    "environment": "composer",
    "name": "weatherbot"
  },
  "MicrosoftAppId": "",
  "publishTargets": [],
  "qna": {
    "hostname": "",
    "knowledgebaseid": "",
    "qnaRegion": "westus"
  },
  "runtime": {
    "command": "dotnet run --project weatherbot.csproj",
    "customRuntime": true,
    "key": "adaptive-runtime-dotnet-webapp",
    "path": "../"
  },
  "runtimeSettings": {
    "adapters": [
      {
        "name": "Microsoft.WebexAdapter",
        "enabled": true,
        "route": "webex",
        "type": "Bot.Builder.Community.Adapters.Webex.WebexAdapter"
      }
    ],
    "features": {
      "removeRecipientMentions": false,
      "showTyping": false,
      "traceTranscript": false,
      "useInspection": false,
      "setSpeak": {
        "voiceFontName": "en-US-JennyNeural",
        "fallbackToTextForSpeechIfEmpty": true
      }
    },
    "components": [
      {
        "name": "Bot.Builder.Community.Adapters.Webex",
        "settingsPrefix": "Bot.Builder.Community.Adapters.Webex"
      }
    ],
    "skills": {
      "allowedCallers": []
    },
    "storage": "",
    "telemetry": {
      "logActivities": true,
      "logPersonalInformation": false,
      "options": {
        "connectionString": ""
      }
    }
  },
  "skillConfiguration": {},
  "Bot.Builder.Community.Adapters.Webex": {
    "WebexAccessToken": "AcessToken",
    "WebexPublicAddress": "https://some.ngrok.io/api/webex",
    "WebexSecret": "secret",
    "route": "webex",
    "type": "Bot.Builder.Community.Adapters.Webex.WebexAdapter",
    "$kind": "Microsoft.WebexAdapter",
    "WebexWebhookName": "webhook"
  },
  "skillHostEndpoint": "http://localhost:3980/api/skills"
